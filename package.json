{
  "name": "prismatic-extension",
  "displayName": "Prismatic",
  "publisher": "prismatic-experimental",
  "private": true,
  "description": "This experimental extension improves the developer experience around Code Native Integrations (CNI) by enabling test execution, integration imports, instance configuration (including config variables, debug mode, and trigger payloads), and inspection of execution results - all directly within the IDE. Developers can view detailed step-by-step outputs and logs for both onTrigger and onExecution stages, streamlining the build, test, and debug workflow without leaving the IDE.",
  "version": "0.0.21",
  "repository": {
    "type": "git",
    "url": "https://github.com/prismatic-io/vscode"
  },
  "icon": "resources/prismatic-icon.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "main": "extension/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "prismatic.configWizard",
        "title": "Config Wizard",
        "category": "Prismatic"
      },
      {
        "command": "prismatic.me",
        "title": "Me",
        "category": "Prismatic"
      },
      {
        "command": "prismatic.me:token",
        "title": "Refresh Token",
        "category": "Prismatic"
      },
      {
        "command": "prismatic.login",
        "title": "Login",
        "category": "Prismatic"
      },
      {
        "command": "prismatic.logout",
        "title": "Logout",
        "category": "Prismatic"
      },
      {
        "command": "prismatic.integrations.import",
        "title": "Import Integration",
        "category": "Prismatic"
      },
      {
        "command": "prismatic.integrations.test",
        "title": "Test Integration",
        "category": "Prismatic",
        "enablement": "prismatic.testCommandEnabled == true"
      },
      {
        "command": "prismatic.prismaticUrl",
        "title": "Prismatic URL",
        "category": "Prismatic"
      }
    ],
    "views": {
      "executionResults": [
        {
          "type": "webview",
          "id": "executionResults.webview",
          "name": "Execution Results"
        }
      ]
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "executionResults",
          "title": "Prismatic",
          "icon": "resources/prismatic.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "prismatic.integrations.import",
          "when": "view == executionResults.webview"
        },
        {
          "command": "prismatic.integrations.test",
          "when": "view == executionResults.webview && prismatic.testCommandEnabled == true"
        }
      ]
    }
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "build:dev": "node esbuild.js",
    "build:prod": "npm run check-types && NODE_ENV=production node esbuild.js",
    "vscode:prepublish": "npm run build:prod",
    "watch": "node esbuild.js --watch",
    "package": "vsce package"
  },
  "dependencies": {
    "@iconscout/react-unicons": "^2.2.5",
    "@msgpack/msgpack": "^3.1.2",
    "@xstate/react": "^5.0.4",
    "date-fns": "^4.1.0",
    "immer": "^10.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.17",
    "xstate": "^5.19.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.12",
    "@types/node": "20.x",
    "@types/vscode": "1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.6.0",
    "esbuild": "^0.25.3",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.2"
  }
}

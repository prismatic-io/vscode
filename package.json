{
  "name": "prismatic-io",
  "displayName": "Prismatic.io",
  "publisher": "prismatic",
  "private": true,
  "description": "This extension improves the developer experience around Code-Native Integrations (CNI) by enabling test execution, integration imports, instance configuration (including config variables, debug mode, and trigger payloads), and inspection of execution resultsâ€”all directly within the IDE.",
  "version": "0.0.26",
  "icon": "resources/prismatic.png",
  "galleryBanner": {
    "color": "#101017",
    "theme": "dark"
  },
  "homepage": "https://github.com/prismatic-io/vscode/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/prismatic-io/vscode"
  },
  "bugs": {
    "url": "https://github.com/prismatic-io/vscode/issues",
    "email": "dev@prismatic.io"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Debuggers",
    "Testing",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "prismatic",
    "integration",
    "cni",
    "code-native integration",
    "debugger",
    "testing",
    "api",
    "workflow",
    "automation"
  ],
  "license": "MIT",
  "main": "extension/extension.js",
  "activationEvents": [
    "workspaceContains:.spectral/*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "prismatic.configWizard",
        "title": "Config Wizard",
        "category": "Prismatic",
        "icon": "$(settings-more-action)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.me",
        "title": "Me",
        "category": "Prismatic",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.refreshToken",
        "title": "Refresh Token",
        "category": "Prismatic",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.login",
        "title": "Login",
        "category": "Prismatic",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.logout",
        "title": "Logout",
        "category": "Prismatic",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.integrations.init",
        "title": "Initialize New Integration",
        "category": "Prismatic",
        "enablement": "prismatic.workspaceEnabled",
        "icon": "$(add)"
      },
      {
        "command": "prismatic.integrations.import",
        "title": "Import Integration",
        "category": "Prismatic",
        "enablement": "prismatic.workspaceEnabled",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "prismatic.integrations.test",
        "title": "Test Integration",
        "category": "Prismatic",
        "enablement": "prismatic.testCommandEnabled && prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.prismaticUrl",
        "title": "Prismatic URL",
        "category": "Prismatic",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.integration.refresh",
        "title": "Refresh Integration",
        "category": "Prismatic",
        "icon": "$(refresh)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.flows.testFlow",
        "title": "Test Flow",
        "category": "Prismatic",
        "icon": "$(play)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.flows.copyWebhookUrl",
        "title": "Copy Webhook URL",
        "category": "Prismatic",
        "icon": "$(copy)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.flows.openInEditor",
        "title": "Open Flow Definition",
        "category": "Prismatic",
        "icon": "$(go-to-file)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.flows.viewDetails",
        "title": "View Flow Details",
        "category": "Prismatic",
        "icon": "$(info)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.flows.testPayload",
        "title": "View/Edit Test Payload",
        "category": "Prismatic",
        "icon": "$(code)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.integration.openInBrowser",
        "title": "Open in Prismatic",
        "category": "Prismatic",
        "icon": "$(link)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.configPages.openInEditor",
        "title": "Open Config Page Definition",
        "category": "Prismatic",
        "icon": "$(go-to-file)",
        "enablement": "prismatic.workspaceEnabled"
      },
      {
        "command": "prismatic.integration.switchTo",
        "title": "Switch to Integration",
        "category": "Prismatic",
        "icon": "$(arrow-swap)",
        "enablement": "prismatic.workspaceEnabled && prismatic.multipleIntegrations"
      },
      {
        "command": "prismatic.integration.quickSwitch",
        "title": "Quick Switch Integration",
        "category": "Prismatic",
        "icon": "$(list-selection)",
        "enablement": "prismatic.workspaceEnabled && prismatic.multipleIntegrations"
      }
    ],
    "views": {
      "prismatic": [
        {
          "id": "prismatic.integration",
          "name": "Integrations",
          "icon": "resources/prismatic.svg",
          "contextualTitle": "Integrations"
        },
        {
          "id": "prismatic.integrationDetails",
          "name": "Integration Details",
          "icon": "resources/prismatic.svg",
          "contextualTitle": "Integration Details"
        }
      ],
      "executionResults": [
        {
          "type": "webview",
          "id": "executionResults.webview",
          "name": "Execution Results",
          "icon": "resources/prismatic.svg",
          "when": "prismatic.workspaceEnabled"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "prismatic.integration",
        "contents": "Welcome to Prismatic!\n\nGet started by creating a new integration or importing an existing one.\n\n[$(add) Initialize New Integration](command:prismatic.integrations.init)\n\n[$(cloud-download) Import Existing Integration](command:prismatic.integrations.import)\n\n[$(book) View Documentation](https://prismatic.io/docs/code-native-integrations-quickstart/)",
        "when": "!prismatic.hasIntegrations"
      },
      {
        "view": "prismatic.integration",
        "contents": "No integrations found in this workspace.\n\n[$(add) Initialize New Integration](command:prismatic.integrations.init)\n\nCreate a new Prismatic integration in this workspace.",
        "when": "prismatic.hasIntegrations && !prismatic.multipleIntegrations && !prismatic.hasActiveIntegration"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "prismatic",
          "title": "Prismatic",
          "icon": "resources/prismatic.svg"
        }
      ],
      "panel": [
        {
          "id": "executionResults",
          "title": "Prismatic",
          "icon": "resources/prismatic.svg",
          "when": "prismatic.workspaceEnabled"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "prismatic.integration.refresh",
          "when": "view == prismatic.integration && prismatic.workspaceEnabled",
          "group": "navigation@2"
        },
        {
          "command": "prismatic.integration.refresh",
          "when": "view == prismatic.integrationDetails && prismatic.workspaceEnabled",
          "group": "navigation@1"
        },
        {
          "command": "prismatic.integrations.import",
          "when": "view == prismatic.integrationDetails && prismatic.workspaceEnabled",
          "group": "navigation@2"
        },
        {
          "command": "prismatic.configWizard",
          "when": "view == prismatic.integrationDetails && prismatic.workspaceEnabled",
          "group": "navigation@3"
        },
        {
          "command": "prismatic.integration.openInBrowser",
          "when": "view == prismatic.integrationDetails && prismatic.workspaceEnabled",
          "group": "navigation@4"
        }
      ],
      "view/item/context": [
        {
          "command": "prismatic.integration.switchTo",
          "when": "view == prismatic.integration && viewItem == integrationImported && prismatic.multipleIntegrations",
          "group": "1_primary@1"
        },
        {
          "command": "prismatic.integration.switchTo",
          "when": "view == prismatic.integration && viewItem == integrationNotImported && prismatic.multipleIntegrations",
          "group": "1_primary@1"
        },
        {
          "command": "prismatic.integrations.import",
          "when": "view == prismatic.integration && viewItem == integrationNotImported",
          "group": "1_primary@2"
        },
        {
          "command": "prismatic.configWizard",
          "when": "view == prismatic.integrationDetails && viewItem == status && prismatic.workspaceEnabled",
          "group": "1_primary@1"
        },
        {
          "command": "prismatic.flows.testFlow",
          "when": "view == prismatic.integrationDetails && viewItem == flow && prismatic.workspaceEnabled",
          "group": "1_primary@1"
        },
        {
          "command": "prismatic.flows.testPayload",
          "when": "view == prismatic.integrationDetails && viewItem == flow && prismatic.workspaceEnabled",
          "group": "1_primary@2"
        },
        {
          "command": "prismatic.flows.copyWebhookUrl",
          "when": "view == prismatic.integrationDetails && viewItem == flow && prismatic.workspaceEnabled",
          "group": "1_primary@3"
        },
        {
          "command": "prismatic.flows.openInEditor",
          "when": "view == prismatic.integrationDetails && viewItem == flow && prismatic.workspaceEnabled",
          "group": "2_navigation@1"
        },
        {
          "command": "prismatic.flows.viewDetails",
          "when": "view == prismatic.integrationDetails && viewItem == flow && prismatic.workspaceEnabled",
          "group": "3_info@1"
        },
        {
          "command": "prismatic.configWizard",
          "when": "view == prismatic.integrationDetails && viewItem == status && prismatic.workspaceEnabled",
          "group": "inline"
        },
        {
          "command": "prismatic.flows.testFlow",
          "when": "view == prismatic.integrationDetails && viewItem == flow && prismatic.workspaceEnabled",
          "group": "inline"
        },
        {
          "command": "prismatic.flows.testPayload",
          "when": "view == prismatic.integrationDetails && viewItem == flow && prismatic.workspaceEnabled",
          "group": "inline"
        },
        {
          "command": "prismatic.flows.copyWebhookUrl",
          "when": "view == prismatic.integrationDetails && viewItem == flow && prismatic.workspaceEnabled",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "build:dev": "node esbuild.js",
    "build:prod": "npm run check-types && NODE_ENV=production node esbuild.js",
    "vscode:prepublish": "npm run build:prod",
    "watch": "node esbuild.js --watch",
    "package": "vsce package",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write --unsafe .",
    "check": "biome check .",
    "check:fix": "biome check --write --unsafe ."
  },
  "dependencies": {
    "@iconscout/react-unicons": "^2.2.5",
    "@msgpack/msgpack": "^3.1.2",
    "@xstate/react": "^5.0.4",
    "date-fns": "^4.1.0",
    "immer": "^10.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.17",
    "xstate": "^5.19.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.12",
    "@types/node": "20.x",
    "@types/vscode": "1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.6.0",
    "esbuild": "^0.25.3",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.2"
  }
}